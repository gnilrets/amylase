<h1>Listing birst_extract_groups</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @birst_extract_groups.each do |birst_extract_group| %>
      <tr>
        <td><%= birst_extract_group.name %></td>
        <td><%= birst_extract_group.description %></td>
        <td><%= link_to 'Show', birst_extract_group %></td>
        <td><%= link_to 'Edit', edit_birst_extract_group_path(birst_extract_group) %></td>
        <td><%= link_to 'Destroy', birst_extract_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Birst extract group', new_birst_extract_group_path %>
