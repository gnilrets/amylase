<% provide(:title, 'Job Summary') %>

<div class="container">
  <div class="row">
    <div class="col-md-8 text-right">
      Show jobs started since:
    </div>
    <div class="col-md-4 text-right">
      <div class="form-group">
        <div class="input-group date" id="min-start-date">
          <input type="text" id="input_min_start_date" data-date-format="YYYY-MM-DD HH:mm:00 UTC" class="form-control" />
          <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span>
          </span>
        </div>
      </div>
    </div>
  </div>
</div>

<table class="table table-hover table-condensed datatable-job-summary">
  <thead>
    <tr>
      <th>id</th>
      <th>Client</th>
      <th>Job Spec</th>
      <th>Start time</th>
      <th>Run time</th>
      <th>Status Message</th>
      <th>Status</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
  </tbody>

  <tfoot>
    <tr>
      <th>id</th>
      <th>Client</th>
      <th>Job Spec</th>
      <th>Start time</th>
      <th>Run time</th>
      <th>Status Message</th>
      <th>Status</th>
      <th></th>
    </tr>
  </tfoot>

</table>

